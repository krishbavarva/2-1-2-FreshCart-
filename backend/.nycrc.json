{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "text-summary",
    "json",
    "html"
  ],
  "exclude": [
    "tests/**",
    "scripts/**",
    "node_modules/**",
    "*.config.js"
  ],
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80
}



